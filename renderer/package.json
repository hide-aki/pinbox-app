{
  "name": "electron-react-ts-starter",
  "description": "This is a boilerplate starter for an electron project that uses react with typescript and has some fixes applied for common bugs.",
  "version": "0.1.0",
  "private": true,
  "author": "ohager",
  "repository": {
    "type": "git",
    "url": "https://github.com/ohager/pinbox-app"
  },
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "electron-is-dev": "^1.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-file-drop": "^0.2.8",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux-starter-kit": "^0.7.0",
    "typescript": "3.5.2"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-prebuild": "cross-env cp ../main/src/*.js public/",
    "start": "cross-env BROWSER=none npm run react-start",
    "build": "cross-env BROWSER=none npm run react-build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^4.3.5",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "electron": "^6.0.9",
    "electron-reload": "^1.4.0",
    "react-intl": "^3.2.3",
    "wait-on": "^3.2.0",
    "watch": "^1.0.2"
  }
}
